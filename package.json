{
  "name": "ai-proxy",
  "version": "1.0.1",
  "description": "Chrome extension that exposes Grok chat as a REST API",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc && npm run copy-assets",
    "server": "node dist/server/localServer.js",
    "dev:all": "npm run build && npm run copy-assets && npm run server",
    "copy-assets": "node scripts/copy-assets.mjs"
  },
  "keywords": [
    "chrome-extension",
    "grok",
    "api",
    "proxy"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.260",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "uuid": "^9.0.1",
    "ws": "^8.18.3"
  }
}
